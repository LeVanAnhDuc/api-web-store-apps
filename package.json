{
  "name": "my-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "dev:check": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit",
    "build": "rimraf ./dist && tsc",
    "start": "yarn build && node dist/app.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts",
    "seed:clear": "ts-node -r tsconfig-paths/register src/database/seeders/index.ts --clear"
  },
  "lint-staged": {
    "**/*.{js,ts,mts,cts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^7.1.0",
    "i18next": "^25.6.1",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.2",
    "joi": "^17.13.3",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.1",
    "nodemailer": "^6.9.14",
    "rate-limit-redis": "^4.3.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.9",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.21",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ua-parser-js": "^0.7.39",
    "@types/winston": "^2.4.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.2.5",
    "nodemon": "^3.1.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.44.0"
  }
}
